use StdIO: all;
use Array: all;

int main()
{
    vect = [1,2,3,4,5,6,7,8,9];
    v = [4];
    print(drop(v,vect));
    print(take((abs(v)/-v) * ((shape(vect) - (v*v/(abs(v))) )),vect));

    n = [-4];
    print(shift (n, 0, vect));
    print(take(shape(vect),take(-shape(vect)-n,vect)));

    return 0;
}



